{
  "name": "graphql-node-typescript-api",
  "version": "1.0.0",
  "description": "API in NodeJs + Typescript + GraphQL",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest",
    "migrations": "ts-node-dev ./node_modules/typeorm/cli.js migration:run"
  },
